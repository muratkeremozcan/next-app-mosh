{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "types": [
      "cypress",
      "node",
      "@bahmutov/cy-api",
      "cypress-map",
      "cypress-v10-preserve-cookie"
    ],
    "baseUrl": "./",
    "paths": {
      "@support/*": ["support/*"],
      "@fixtures/*": ["fixtures/*"],
      "@/*": ["../*"]
    },
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts*", "../cypress.d.ts"],
  "extends": "../tsconfig.json"
}
